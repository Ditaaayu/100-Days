import java.util.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("baris : ");
		int a=sc.nextInt();
		System.out.print("kolom : ");
		int b=sc.nextInt();
		
		for(int i=0;i<a;i++){
			for(int j=0;j<b;j++){
				System.out.print("*");
				
			}
			System.out.println();
		}	
		
	}
}

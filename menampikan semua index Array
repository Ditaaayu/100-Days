package pkg100day_ngoding;

import java.util.Scanner;

public class Day_54 {

    public static void main(String[] args) {
//       array 
        Scanner sc = new Scanner(System.in);
        int[] array = {3, 7, 4, 2, 8, 6, 1, 6, 7};
        int x = sc.nextInt();

        for (int i = 0; i < array.length; i++) {
            if (x == array[i]) {
                System.out.println("posisi : " + i);
            }
        }
    }
}



package pkg100day_ngoding;

public class Day_46 {

    public static void main(String[] args) {
//        untuk menampilkan bilangan ganjil dari 1 hingga 20
       
        for (int i = 1; i < 20; i++) {
            if (i % 2 != 0) {

                System.out.print(i+" ");
            }
        }
    }

}

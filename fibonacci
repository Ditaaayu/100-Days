package pkg100day_ngoding;

public class Day_37 {

    public static void main(String[] args) {
        int n = 10;
        for (int i = 1; i < n; i++) {
            System.out.print(fibonacci(i) + " ");
        }
    }

    public static int fibonacci(int n) { //method rekursif
        if (n <= 1) {
            return n;
        } else {
            return fibonacci(n - 1) + fibonacci(n - 2);
        }
    }
}
